public class Main {
  public static void main(String[] args) {
    
  float a = 3.5f;
  float b = 11.25f;
  
    System.out.println(a+b);
    System.out.println(a-b);
    System.out.println(a*b);
    System.out.println(a/b);
    System.out.println(a%b); 
    System.out.println(a>b);
    System.out.println(a>=b); 
    System.out.println(a<b);
    System.out.println(a<=b);  
    System.out.println(a!=b); 
    System.out.println(a==b);
   
    
 }
}
